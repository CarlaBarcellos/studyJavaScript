{
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            2
        ],
        "no-console": [0],
        // "no-var": ["error"],
        "quotes": [ "error", "single" ],
        "semi": [ "error", "always" ]
    }
}